    export WIKIEMAIL=$1
    USERNAME="wikitester@ken.socialtext.net"
    alias tail='tail'
    sudo -u www-data ./run-wiki-tests --no-maximize --test-username $USERNAME --test-email $USERNAME  --plan-server http://$HOSTNAME  --test-server http://$HOSTNAME  --timeout 60000 --plan-page "OSR Testcases" >&  testcases.out & 
    echo ""
    echo RUNNING OSR Testcases to testcases.out on host $HOSTNAME
    echo use tail -f testcases.out to obseve the test run
    echo ""
