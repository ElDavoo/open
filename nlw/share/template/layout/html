    <head>
        <title>[% title || display_title %] / [% wiki.title %]</title>
        [% PROCESS element/css_links %]
        [% IF feeds %]
            [% IF feeds.rss %]
                [% FOREACH feed IN feeds.rss %]
        <link rel="alternate" type="application/rss+xml" title="[% feed.value.title | html_encode %]" href="[% feed.value.url %]" />
                [% END %]
            [% END %]

            [% IF feeds.atom %]
                [% FOREACH feed IN feeds.atom %]
        <link rel="alternate" type="application/atom+xml" title="[% feed.value.title | html_encode %]" href="[% feed.value.url %]" />
                [% END %]
            [% END %]
        [% END %]


        [% INCLUDE "element/wiki/js-bootstrap" %]

        [% combined_file = 'socialtext-other' %]
        [% IF page_action == 'display' %]
            [% combined_file = "socialtext-display" %]
        [% END %]
        <script type="text/javascript" charset="utf-8"
            src="[% wiki.static_path %]/javascript/[% combined_file %].js.gz">
        </script>
        [% IF customjs_name %]
        <script type="text/javascript" charset="utf-8" src="[% wiki.static_path %]/javascript/[% customjs_name %]/custom.js"></script>
        [% ELSIF customjs_uri %]
        <script type="text/javascript" charset="utf-8" src="[% customjs_uri %]"></script>
        [% END %]

        <meta name="keywords" content="[% loc('Confluence wiki, Enterprise 2.0 wiki, Mediawiki, Microsoft wiki, Sharepoint wiki, Twiki, Web 2.0 wiki, business wiki, document management, enterprise wiki, intranet wiki, knowledge management, online collaboration, open source wiki, portal wiki, hosted wiki, project collaboration, team collaboration, wiki appliance, wiki software') %]"/>

    </head>
    <body id="socialtext-wiki">
