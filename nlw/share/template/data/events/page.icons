[%- SWITCH event.action -%]
    [%- CASE "comment" -%]<img class="asset-icon" src="/static/images/icons/conversation-black-25.png" />
    [%- CASE "tag_add" -%][%# no icon yet %]
    [%- CASE "tag_delete" -%][%# no icon yet %]
    [%- CASE "watch_add" -%]<img class="asset-icon" src="/static/images/icons/watch-black-25.png" />
    [%- CASE "watch_delete" -%]<img class="asset-icon" src="/static/images/icons/watch-black-25.png" />
    [%- CASE "like" -%]<img class="asset-icon" src="/static/images/icons/thumbsup-black-25.png" />
    [%- CASE "unlike" -%]<img class="asset-icon" src="/static/images/icons/thumbsup-black-25.png" />
    [%- CASE -%]<img class="asset-icon" src="/static/images/icons/edit-black-25.png" />
[%- END -%]

