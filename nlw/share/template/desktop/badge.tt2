function encodeToHex(str) {
    var res = '';
    for (var c = 0; c < str.length; c++) {
        var hex = str.charCodeAt(c).toString(16);
        if (hex.length < 2) res += '0';
        res += hex;
    }
    return res;
}

var server = encodeToHex(
    location.protocol + '//' + location.hostname
    + ((location.port == 80) ? '' : ':' + location.port)
);
var so = new SWFObject("/static/desktop/AIRInstallBadge.swf", "", "215", "180", "9.0.115", "#000000");

so.addVariable("airversion", "1.5");
so.addVariable("appname", "[% app_name %]");
so.addVariable("appurl", '[% app_url %]');
so.addVariable("appid", "[% app_id %]");
so.addVariable('appinstallarg', server);
so.addVariable('applauncharg', server);
so.addVariable("pubid", "[% pub_id %]");
so.addVariable("appversion", "[% app_version %]");
so.addParam("allowScriptAccess", "always");

var prevHTML;
function hide_swfobject () {
    $('#flashcontent').html(prevHTML);
    $('#getThings').show();
    $('.getAIR').show();
    $('.getFlash').hide();
}

function hasFlash(selector) {
    if ($(selector).length == 0) return false;
    var obj = $(selector).get(0);
    if (typeof obj == 'undefined') return false;
    return(typeof obj.GetVariable != 'undefined');
}

function tryInit (tries) {
    if (hasFlash('#airDetect') || hasFlash('#airDetectIE')) {
        so.write("flashcontent");
        $('#st-desktop-close').removeClass('disabled');
        $('#getThings').show();
    }
    else if (tries > 10) {
        so.addVariable("airswfurl", '[% air_swf_url %]');
        so.write("flashcontent");
        $('#st-desktop-close').removeClass('disabled');
        $('#getThings').show();
    }
    else {
        tries++;
        setTimeout(function(){ tryInit(tries) }, 250);
    }
}

$(function(){
    prevHTML = $('#flashcontent').html();
    setTimeout(function(){ tryInit(0) }, 250);
});

</script>
</body></html>
