                            [% ELSE %]
                            <li class="last"><a href="/nlw/submit/logout[% IF wiki.is_public %]?redirect_to=[% wiki.uri | html %][% END %]">[% loc('Log out') %]</a></li>
                            [% END %]
                        </ul>
                    </div><!-- footerGlobal END -->
                </div><!-- footer END -->
