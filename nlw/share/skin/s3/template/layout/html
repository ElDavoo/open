                            <input type="hidden" value="search" name="action"/>
                            <input type="hidden" value="_" name="scope"/>
                            <input id="st-search-term" name="search_term" class="searchText" type="text" />
                            <input id="st-search-submit" class="searchButton"  type="button" />
                        </form>
                    </div>
                </div><!-- globalNav END -->
            </div><!-- header END -->
            <div id="mainNav" class="st-wiki-subnav-dashboard">
                <div id="homeLink"><a id="st-home-link" href="[% home_href | decorate('home_href') %]">[% loc("Home") %]</a></div>
